RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} ^diogonunes.com [NC]
RewriteRule ^(.*)$ http://www.diogonunes.com/$1 [L,R=301]

# DO NOT REMOVE THIS LINE AND THE LINES BELOW REDIRECTID:uR1XFB
RewriteEngine on
RewriteCond %{HTTP_HOST} ^diogonunes.com$ [OR]
RewriteCond %{HTTP_HOST} ^www.diogonunes.com$
RewriteRule ^blog$ http://vozdamente.blogspot.com [R=301,L]
